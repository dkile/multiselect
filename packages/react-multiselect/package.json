{
  "name": "@dkile/react-multiselect",
  "version": "0.0.11",
  "description": "React hook wrapper for multiselect-core",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkile/multiselect.git",
    "directory": "packages/react-multiselect"
  },
  "keywords": [
    "react",
    "multiselect",
    "hook",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "dependencies": {
    "@dkile/multiselect-core": "^0.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "tsup": "^8.5.0",
    "typescript": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
